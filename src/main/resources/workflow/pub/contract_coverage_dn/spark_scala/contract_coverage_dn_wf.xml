<?xml version="1.0" encoding="UTF-8"?>
<workflow-app xmlns="uri:oozie:workflow:0.4" name="contract-coverage-dn-spark-scala-wf">
   <global>
     <job-tracker>${job_tracker}</job-tracker>
     <name-node>${name_node}</name-node>
     <configuration>
       <property>
         <name>tez.queue.name</name>
         <value>${queue_name}</value>
       </property>
       <property>
         <name>mapreduce.job.queuename</name>
         <value>${queue_name}</value>
       </property>
     </configuration>
   </global>
   <credentials>
     <credential name="cred" type="hcat">
       <property>
         <name>hcat.metastore.principal</name>
         <value>${metastorePrincipal}</value>
       </property>
       <property>
         <name>hcat.metastore.uri</name>
         <value>${metaStoreUri}</value>
       </property>
     </credential>
   </credentials>
   <start to="d8-es-contract-coverage-ld-1-ins" />
   <action cred="cred" name="d8-es-contract-coverage-ld-1-ins">
     <spark xmlns="uri:oozie:spark-action:0.2">
       <job-tracker>${job_tracker}</job-tracker>
       <name-node>${name_node}</name-node>
       <master>${master}</master>
       <mode>${mode}</mode>
       <name>d8-es-contract-coverage-ld-1-ins</name>    
       <class>com.ncr.edl.apollo.contract_coverage_dn.d8escontractcoverageld1insRunner</class>
       <jar>${jarHDFSPath}</jar>
       <spark-opts>--queue ${queue_name} --properties-file user.properties ${sparkConfCustom}</spark-opts>
       <arg>${dbParamFileHDFSPath}</arg>
       <arg> ${activityCountRequired}</arg>
     </spark>
     <ok to="d8-es-coverage-dn-t01-ins" />
     <error to="fail" />
   </action>
   <action cred="cred" name="d8-es-coverage-dn-t01-ins">
     <spark xmlns="uri:oozie:spark-action:0.2">
       <job-tracker>${job_tracker}</job-tracker>
       <name-node>${name_node}</name-node>
       <master>${master}</master>
       <mode>${mode}</mode>
       <name>d8-es-coverage-dn-t01-ins</name>    
       <class>com.ncr.edl.apollo.contract_coverage_dn.d8escoveragednt01insRunner</class>
       <jar>${jarHDFSPath}</jar>
       <spark-opts>--queue ${queue_name} --properties-file user.properties ${sparkConfCustom}</spark-opts>
       <arg>${dbParamFileHDFSPath}</arg>
       <arg> ${activityCountRequired}</arg>
     </spark>
     <ok to="d8-coverage-dn-01-ins" />
     <error to="fail" />
   </action>
   <action cred="cred" name="d8-coverage-dn-01-ins">
     <spark xmlns="uri:oozie:spark-action:0.2">
       <job-tracker>${job_tracker}</job-tracker>
       <name-node>${name_node}</name-node>
       <master>${master}</master>
       <mode>${mode}</mode>
       <name>d8-coverage-dn-01-ins</name>    
       <class>com.ncr.edl.apollo.contract_coverage_dn.d8coveragedn01insRunner</class>
       <jar>${jarHDFSPath}</jar>
       <spark-opts>--queue ${queue_name} --properties-file user.properties ${sparkConfCustom}</spark-opts>
       <arg>${dbParamFileHDFSPath}</arg>
       <arg> ${activityCountRequired}</arg>
     </spark>
     <ok to="end" />
     <error to="fail" />
   </action>
   <kill name="fail">
      <message>Error while running workflow contract-coverage-dn-spark-scala-wf workflow: [${wf:errorMessage(wf:lastErrorNode())}]</message>
   </kill>
   <end name="end" />
</workflow-app>
